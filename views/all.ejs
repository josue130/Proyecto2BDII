<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"> 
    <link rel="stylesheet" href="resources/css/style.css"

</head>
<body>
    <header id = "mainHeader">
        <nav class="navbar navbar-expand-lg navbar navbar-dark bg-dark">
            <div class="container-fluid">
              <a class="navbar-brand" href="#">Wiki-Words</a>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  <li class="nav-item">
                    <a class="btn btn-outline-success" aria-current="page" href="/main">Home</a>
                    <!-- nav-link active -->
                  </li>
                  
                  
                </ul>

                
              </div>
            </div>
          </nav>

    </header>
    <section>
        <article id="main-content3">
            <Header>
                <h1>Results</h1>
                <table id ="main-content4">
                    <thead>
                      <tr>
                        <th>Page</th>
                        <th>Word</th>
                        <th>Quantity</th>
                      </tr>
                    </thead>
                    <tbody>
                    
                      <% all.forEach((transfer)=> { %>
                        <tr>
                          <td><%= transfer.pagina %></td>
                          <td><%= transfer.palabra %></td>
                          <td><%= transfer.cantidad %></td>
                        </tr>
                      <% }) %>
                      
                    </tbody>
                    <article >
                        <table id ="main-content5">
                          <thead>
                            <tr>
                              <th>Word</th>
                              <th>% h1 </th>
                              <th>% h2 </th>
                              <th>% h3 </th>
                              <th>% p </th>
                              <th>% li </th>
                            </tr>
                          </thead>
                          <tbody>
                          
                            <% all2.forEach((transfer)=> { %>
                              <tr>
                                <td><%= transfer.palabra %></td>
                                <td><%= transfer.PorcentajeH1 %></td>
                                <td><%= transfer.PorcentajeSubH2 %></td>
                                <td><%= transfer.PorcentajeSubH3 %></td>
                                <td><%= transfer.parrafo %></td>
                                <td><%= transfer.PorcentajeLI %></td>
                              </tr>
                            <% }) %>
                            
                          </tbody>
                        </table>
                      </article>
            </Header>
        </article>
    </section>

    
</body>
</html>